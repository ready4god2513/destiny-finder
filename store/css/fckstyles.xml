<Styles>
	<Style name="Image on Left" element="img">
		<Attribute name="style" value="padding: 5px; margin-right: 5px" />
		<Attribute name="border" value="2" />
		<Attribute name="align" value="left" />
	</Style>
	<Style name="Image on Right" element="img">
		<Attribute name="style" value="padding: 5px; margin-left: 5px" />
		<Attribute name="border" value="2" />
		<Attribute name="align" value="right" />
	</Style>
	<Style name="Standard Text" element="span">
		<Attribute name="class" value="mainpage" />
	</Style>
	<Style name="Small Text" element="span">
		<Attribute name="class" value="smalltext" />
	</Style>
	<Style name="Section Header" element="span">
		<Attribute name="class" value="header" />
	</Style>
	<Style name="Section Title" element="span">
		<Attribute name="class" value="section_title" />
	</Style>
	<Style name="Section Footer" element="span">
		<Attribute name="class" value="section_footer" />
	</Style>
	<Style name="Title" element="span">
		<Attribute name="class" value="cat_title_large" />
	</Style>
	<Style name="Title Sm" element="span">
		<Attribute name="class" value="cat_title_small" />
	</Style>
	<Style name="Title List" element="span">
		<Attribute name="class" value="cat_title_list" />
	</Style>
	<Style name="Text Lg" element="span">
		<Attribute name="class" value="cat_text_large" />
	</Style>
	<Style name="Text Featured" element="span">
		<Attribute name="class" value="cat_text_featured" />
	</Style>
	<Style name="Text Sm" element="span">
		<Attribute name="class" value="cat_text_small" />
	</Style>
	<Style name="Text List" element="span">
		<Attribute name="class" value="cat_text_list" />
	</Style>
</Styles>